 static uint8_t lcd12864_read_data(uint8_t chip_select)
 {
 uint16_t timeout;
 uint8_t dsp_data;
 
 timeout = 0;
 while(lcd12864_is_busy()){
 timeout++;
 if(timeout > 0xff){
  ;
 break;
 }
 }
  P0 = 0xFF;
 if(chip_select == 1){
 LCD12864_CS2 = 0;
 LCD12864_CS1 = 1;
 }
 else{
 LCD12864_CS2 = 1;
 LCD12864_CS1 = 0;
 }
 LCD12864_RW = 1;
 LCD12864_DI = 1;
 LCD12864_EN = 1;
 LCD12864_EN = 0;
 LCD12864_EN = 1; 
 dsp_data = P0;
 LCD12864_EN = 0;
 
 return dsp_data;
 }
 











 

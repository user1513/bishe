 static bool lcd12864_is_busy(void)
 {
 uint8_t state = 0;
 bool busy;
 
  P0 = 0xFF;
 LCD12864_RW = 1;
 LCD12864_DI = 0;
 LCD12864_EN = 1;
 LCD12864_EN = 0;
 LCD12864_EN = 1; 
 state = P0;
 LCD12864_EN = 0;
 
 busy = (bool)(state>>7);
 return busy; 
 }
 











 
